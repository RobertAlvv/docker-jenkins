pipeline {
    agent any
    
    stages {
        stage('Build') {
            steps {
                sh 'echo "Compiling code..."'
            }
        }
        
        stage('Test') {
            steps {
                sh 'echo "Running tests..."'
            }
        }
        
        stage('Deploy') {
            steps {
                sh 'echo "Deploying code..."'
            }
        }
    }
}